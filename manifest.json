{
  "manifest_version": 3,
  "name": "FastProxy - 智能代理切换",
  "version": "5.3.0",
  "description": "高效的代理自动分流工具。支持 GFWList、自定义黑/白名单 (直连)、PAC 脚本、配置备份及延迟测试。是 SwitchyOmega 的轻量化替代方案。",
  "permissions": [
    "proxy",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "background": {
    "service_worker": "js/background.js" 
  },
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": "assets/icon.png"
  },
  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": true
  }
}