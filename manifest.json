{
  "manifest_version": 3,
  "name": "FastProxy - 专业版",
  "version": "5.0.0",
  "description": "极简面板 + 独立设置中心",
  "permissions": [
    "proxy",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}